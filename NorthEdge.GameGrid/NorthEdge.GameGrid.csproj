<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net8.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <AssemblyName>NorthEdge.GameGrid</AssemblyName>
        <RootNamespace>NorthEdge.GameGrid</RootNamespace>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <Title>Generic game grid</Title>
        <Authors>NorthEdge</Authors>
        <Description>A generic game grid</Description>
        <PackageProjectUrl>https://github.com/North-Edge/gamegrid</PackageProjectUrl>
        <RepositoryUrl>https://github.com/North-Edge/gamegrid</RepositoryUrl>
    </PropertyGroup>

</Project>
